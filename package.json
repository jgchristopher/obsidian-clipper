{
	"name": "obsidian-clipper",
	"version": "0.2.8",
	"description": "This plugin helps you capture highlights from the web.",
	"main": "main.js",
	"scripts": {
		"compile": "tsc -noEmit -skipLibCheck",
		"compile:bookmarklet": "tsc -skipLibCheck ./src/build/bookmarkletcode/index.ts",
		"build:bookmarklet": "npm run compile:bookmarklet && npm run webpack && ./node_modules/.bin/bookmarklet ./src/build/bookmarkletcode/dist/bookmarklet.js ./src/build/bookmarkletcode/dist/obsidian-clipper.min.js",
		"build:bookmarkletgen": "node ./src/bookmarkletlink/build-bookmarkletgenerator.js",
		"tailwindcss": "npx tailwindcss -i ./input.css -o ./styles.css",
		"version": "node ./src/build/version-bump.mjs && git add manifest.json versions.json",
		"webpack": "webpack",
		"build": "npm run build:bookmarklet && npm run build:bookmarkletgen && npm run compile && npm run tailwindcss &&  node esbuild.config.mjs production",
		"dev": "npm run build && cp main.js styles.css manifest.json ~/gitprojects/obsidian-plugins/obsidian-clipper-projects/dev/",
		"clean": "rimraf ./src/build/bookmarkletcode/index.js && rimraf ./src/build/bookmarkletcode/obsidian-clipper.min.js && rimraf ./src/build/bookmarkletcode/dist/*"
	},
	"keywords": [],
	"author": "jgchristopher",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "3.0.0",
		"@types/node": "16.11.6",
		"@types/turndown": "5.0.1",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"bookmarklet": "^3.0.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"esbuild-svelte": "^0.7.3",
		"eslint": "^8.34.0",
		"jsdom": "^22.1.0",
		"obsidian": "obsidianmd/obsidian-api#master",
		"rimraf": "^4.4.1",
		"svelte": "3.55.1",
		"svelte-eslint-parser": "^0.23.0",
		"svelte-popperjs": "^1.3.2",
		"svelte-preprocess": "5.0.1",
		"svelte-writable-derived": "^3.1.0",
		"tailwindcss": "^3.2.7",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vitest": "^0.31.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1",
		"moment": "^2.29.4"
	},
	"dependencies": {
		"@dagrejs/dagre": "^1.0.2",
		"@types/dagre": "^0.7.48",
		"deepmerge-ts": "^4.3.0",
		"obsidian-daily-notes-interface": "0.9.4",
		"parse-domain": "^7.0.1",
		"turndown": "7.1.1"
	}
}
