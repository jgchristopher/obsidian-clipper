{
	"name": "obsidian-clipper",
	"version": "0.0.5",
	"description": "This plugin helps you capture highlights from the web.",
	"main": "main.js",
	"scripts": {
    "webpack": "npx webpack",
		"dev": "node esbuild.config.mjs",
		"compile": "tsc -noEmit -skipLibCheck",
    "build:compileBookmarklet": "tsc src/bookmarklet/index.ts",
		"build:bookmarklet": "npm run build:compileBookmarklet  && ./node_modules/.bin/bookmarklet src/bookmarklet/dist/bookmarklet.js src/bookmarklet/obsidian-clipper.min.js",
		"build:bookmarkletgen": "node replace-bookmarklet-code.js",
		"build": "npm run build:bookmarklet && npm run build:bookmarkletgen && npm run compile && npm run tailwindcss &&  node esbuild.config.mjs production",
		"tailwindcss": "npx tailwindcss -i ./input.css -o ./styles.css",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "jgchristopher",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "3.0.0",
		"@types/node": "16.11.6",
		"@types/turndown": "5.0.1",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"bookmarklet": "^3.0.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"esbuild-svelte": "^0.7.3",
		"eslint": "^8.34.0",
		"micromodal": "^0.4.10",
		"obsidian": "latest",
		"svelte": "3.55.1",
		"svelte-eslint-parser": "^0.23.0",
		"svelte-popperjs": "^1.3.2",
		"svelte-preprocess": "5.0.1",
		"tailwindcss": "^3.2.7",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"webpack": "^5.75.0",
		"webpack-cli": "^5.0.1"
	},
	"dependencies": {
		"deepmerge-ts": "^4.3.0",
		"obsidian-daily-notes-interface": "0.9.4",
		"turndown": "7.1.1"
	}
}
